 import math

 def paint_calc(height, width, cover):
   num_cans = (height * width)/cover
   rounded_cans = math.ceil(num_cans)
   print(f"You'll need {rounded_cans} cans of paint.")
  

 w = int(input("Enter width of the wall: "))
 h = int(input("Enter height of the wall: "))
 coverage = 5

 paint_calc(height=h, width=w, cover=coverage)
