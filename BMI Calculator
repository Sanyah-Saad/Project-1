height = float(input("Enter your height."))
weight =float(input("Enter your weight."))
bmi = weight/ height**2  
if bmi < 18.5:  
 print("You're under weight")

elif bmi < 25: 
 print("Your weight is normal")

elif bmi <30:
 print("You're over weight")

elif bmi <35:
 print("You're obese")

else:
 print("you're clinically obese")
