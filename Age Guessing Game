print("Welcome!")

while True:
  try:
      age = float(input("What is your age? "))
      break  
  except ValueError:
      print("Error: Please enter a numeric value.")  


if age <= 12:
        print("You are a child")
elif age <= 19:
        print("You are a teenager")
elif age <= 64:
        print("You are an adult")
elif age > 65:
     print("You are a senior")

